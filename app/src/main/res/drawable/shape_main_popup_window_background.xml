<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        dashWidth指的是边线的长度;
        dashGap指的是每条线之间的间距;
        width指的是边线的宽度; -->
    <solid android:color="#444444"/>
    <stroke
        android:width="2dp"
        android:color="#00AAEE"
        android:dashGap="0dp"
        android:dashWidth="0dp" />
    <corners android:radius="2dp" />
</shape>